{
    "name": "Crunshitify",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "Crunchyroll stuck in 2008",
    "homepage_url": "https://www.github.com/jasonfran",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "permissions": [
      "https://www.crunchyroll.com/*"
    ],
    "content_security_policy": "default-src 'self';",
    "content_scripts": [
      {
        "matches": [
          "https://www.crunchyroll.com/*"
        ],
        "js": [
          "js/content.js"
        ]
      }
    ]
  }
  
